{
  "name": "casaPat",
  "version": "1.0.0",
  "description": "Air quality sensor plugin for Homebridge",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint src/**/*.js --max-warnings=0",
    "lint:fix": "eslint src/**/*.js --fix",
    "watch": "npm run build && sudo hb-service link && nodemon",
    "build": "rimraf ./dist && babel src --out-dir dist",
    "prepublishOnly": "npm run build",
    "postinstall": "npm run build"
  },
  "keywords": [
    "homebridge-plugin"
  ],
  "engines": {
    "node": ">=18.20.3",
    "homebridge": ">=1.3.0"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.28.6",
    "node-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "eslint": "^9.6.0",
    "homebridge": "^1.8.3",
    "nodemon": "^3.1.4",
    "rimraf": "^5.0.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mremigio331/WALLE-HomebridgePlatform.git"
  }
}
